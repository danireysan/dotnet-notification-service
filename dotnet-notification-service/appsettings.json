{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=${POSTGRES_DB};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD}"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5143"
      }
    }
  },
  "Jwt": {
    "Key": "${TOKEN_SECRET}",
    "Issuer": "dotnet-notification-service",
    "Audience": "dotnet-notification-service-audience",
    "ExpiresMinutes": 30
  }
}